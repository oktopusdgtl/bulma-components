{
  "name": "bulma-webcomponents",
  "version": "0.1.4",
  "description": "Webcomponents version of the famous Bulma css framework",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/bulma-webcomponents/bulma-webcomponents.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oktopusdgtl/bulma-components.git"
  },
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^1.3.3",
    "@stencil/sass": "^1.1.1"
  },
  "license": "MIT",
  "keywords": [
    "bulma",
    "webcomponent",
    "custom element",
    "light dom",
    "stenciljs"
  ],
  "dependencies": {
    "@stencil/router": "^1.0.1",
    "bulma": "^0.8.0"
  }
}
